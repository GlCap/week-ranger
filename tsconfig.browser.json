{
  "extends": "./tsconfig",
  "compilerOptions": {
    "outDir": "./dist/browser",
    "target": "ES6",
    "moduleResolution": "node",
    "lib": ["ES6", "dom", "ES5"],
    "downlevelIteration": true,
    "declaration": false,
    "declarationMap": false,
    "inlineSourceMap": true
  },
  "exclude": ["dist", "**/__tests__/**"]
}
